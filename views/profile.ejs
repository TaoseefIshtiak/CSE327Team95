<h1> User Update </h1>

<form action="/profile" method="POST">
    <input type = "text" name= "firstName" placeholder = "firstName" value=<%= infos.firstName %>>
    <input type = "text" name= "lastName" placeholder = "lastName" value=<%= infos.lastName %>>
    <input type = "text" name= "username" placeholder = "username" value=<%= infos.username %>>
    <input type = "text" name= "eMail" placeholder = "youName@domainName.com" value=<%= infos.eMail %>>
    <button>Submit</button>
</form>